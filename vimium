# Vimium for Chrome/Chromium config backup

# Custom key mappings

map ' Marks.activateGotoMode

map go LinkHints.activateModeToOpenInNewForegroundTab
map gm LinkHints.activateModeWithQueue
map gd LinkHints.activateModeToDownloadLink
map ga LinkHints.activateModeToOpenIncognito

map l nextTab
map h previousTab
map U restoreTab
map < moveTabLeft
map > moveTabRight
map gX closeTabsOnLeft
map gx closeTabsOnRight
map gO closeOtherTabs
map B activateBookmarks

map <a-h> scrollLeft
map <a-l> scrollRight

map ]f nextFrame
map [f mainFrame

unmap t
unmap b
unmap <a-f>
unmap <c-e>
unmap <c-y>
unmap gt
unmap gT
unmap K
unmap J
unmap X
unmap <<
unmap >>

# Characters used for link hints
asdfjklewioghpqurmvnb

# Default search engine
http://global.bing.com/search?setmkt=en-us&setlang=en-us&q=
